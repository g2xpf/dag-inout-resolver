// dotファイルはdigraphまたはgraphで始めます
// graph_nameはこの例でのグラフ名です。
digraph simple_graph {
  layout=neato;
  a [pos="-2, 3!"];
  b [pos="-1, 3!"];
  c [pos="0, 3!"];
  d [pos="1, 3!"];
  e [pos="2, 3!"];
  f, g, h, j, k, l;
  m [pos="-2, -3!"];
  n [pos="0, -3!"];
  o [pos="2, -3!"];
  a -> f;
  b -> f;
  c -> h;
  d -> i;
  e -> j;
  f -> g;
  f -> l;
  f -> m;
  g -> h;
  g -> l;
  g -> k;
  h -> j;
  h -> k;
  i -> j;
  j -> k;
  l -> n;
  k -> o;
}
